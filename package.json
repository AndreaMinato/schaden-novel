{
  "name": "schaden-novel",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "import:all": "node ./scripts/import.mjs",
    "import:docs": "node ./scripts/import_ids.mjs",
    "deploy": "pnpm build && netlify deploy --prod --dir=dist --no-build",
