{
  "name": "schaden-novel",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "nuxi dev",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 nuxi generate",
    "preview": "npx serve .output/public",
    "deploy": "pnpm build && netlify deploy --prod --dir=.output/public --no-build",
    "import:all": "node ./scripts/import.mjs",
    "import:docs": "node ./scripts/import_ids.mjs",
    "git:sync": "git add ./src/content && git commit -m 'update' && git push && git checkout .",
    "publish": "pnpm import:docs && pnpm git:sync && pnpm run deploy"
  },
  "dependencies": {
    "nuxt": "latest",
    "@nuxt/content": "latest",
    "@nuxt/ui": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "cheerio": "^1.1.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild", "@parcel/watcher", "vue-demi"]
  }
}
